{
    "name": "frontend-angular",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "test": "ng test",
        "build": "check-audit && npm run lint && npm run test-no-watch && npm run e2e && ng build --aot=true",
        "build-prod": "check-audit && npm run lint && npm run test-no-watch && npm run e2e && rimraf dist && ng build --prod",
        "lint": "ng lint && prettier-check \"./@(src|e2e)/**/*@(.ts|.js|.css|.html)\"",
        "test-no-watch": "ng test --watch=false",
        "resolve-audit": "resolve-audit",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.2.9",
